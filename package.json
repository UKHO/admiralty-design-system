  "scripts": {
    "ng": "ng",
    "start": "yarn storybook",
    "build": "ng build && yarn build-styles",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && cross-env BROWSER=none start-storybook -s ./assets -c .storybook -p 9009",
    "build-storybook": "yarn docs:json && build-storybook -s ./assets",
    "build-styles": "node styles/generateCSS.js && postcss dist/styles/css/ukho.min.css --map --use autoprefixer -d dist/styles/css"
  },
  "peerDependencies": {
    "@angular/cdk": ">=9.x && <=11.x",
