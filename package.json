    "@angular/platform-browser": ">=9.x && <=11.x",
    "@fortawesome/fontawesome-free": "5.x",
    "zone.js": ">=0.10.x && <=0.11.x"
  },
  "dependencies": {
    "filesize": "^6.1.0",
    "highlight.js": "^10.7.2",
