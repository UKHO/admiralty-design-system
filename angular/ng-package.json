{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "dest": "dist",
  "assets": ["styles/!(generateCSS.js)", "./assets"],
  "lib": {
    "entryFile": "components/public-api.ts",
    "styleIncludePaths": ["styles/core.scss"],
    "umdModuleIds": {
      "filesize": "filesize",
      "highlight.js": "highlight.js"
    }
  },
  "whitelistedNonPeerDependencies": ["filesize", "highlight.js", "normalize.css"]
}
