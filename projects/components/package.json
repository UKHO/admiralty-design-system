{
  "name": "@ukho/components",
  "version": "1.2.6",
  "description": "Angular components library for the UKHO Design System",
  "author": {
    "name": "UK Hydrographic Office",
    "url": "https://www.gov.uk/ukho"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHO/design-system.git"
  },
  "scripts": {
    "prepare": "ng build --prod && cpx ./src/scss/core.scss dist/",
    "storybook": "start-storybook -s node_modules/@ukho/styles,node_modules/@fortawesome/fontawesome-free",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "@ukho/styles": "^1.3.6"
  },
  "peerDependencies": {
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~9.0.0-next.8",
    "@angular/core": "~9.0.0-next.8",
    "@fortawesome/fontawesome-free": "^5.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/angular": "^5.2.1",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "raw-loader": "1.0.0"
  }
}
