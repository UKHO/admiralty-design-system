{
  "name": "@ukho/components",
  "version": "1.5.3",
  "description": "Angular components library for the UKHO Design System",
  "author": {
    "name": "UK Hydrographic Office",
    "url": "https://www.gov.uk/ukho"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHO/design-system.git"
  },
  "scripts": {
    "prepare": "ng build --prod && cpx ./src/scss/core.scss dist/",
    "storybook": "yarn docs:json && start-storybook -s node_modules/@ukho/styles,node_modules/@fortawesome/fontawesome-free,src/assets",
    "build-storybook": "yarn docs:json && build-storybook -c .storybook -o .out  -s node_modules/@ukho/styles,node_modules/@fortawesome/fontawesome-free,src/assets",
    "docs:json": "compodoc -p ./tsconfig.lib.json -e json -d ."
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "filesize": "^5.0.3"
  },
  "peerDependencies": {
    "@angular/cdk": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@ukho/styles": "~1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/angular": "^5.3.14",
    "@ukho/styles": "~1.5.0",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "raw-loader": "1.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-resources-loader": "^2.0.1",
    "sb": "^5.1.11",
    "typescript": "~3.7.0"
  },
  "gitHead": "323df1a6ba1c4d2952dd97449cd80a65a6c07a4a"
}
