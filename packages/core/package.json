{
  "name": "@ukho/admiralty-core",
  "version": "3.1.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/admiralty-core/admiralty-core.esm.js",
  "files": [
    "dist/",
    "loader/",
    "styles/",
    "docs/",
    "themes/"
  ],
  "scripts": {
    "build": "yarn run build.css && stencil build",
    "build.css": "yarn run css.sass && yarn run css.minify",
    "css.minify": "cleancss -O2 -o styles/admiralty.bundle.css styles/admiralty.bundle.css",
    "css.sass": "sass --embed-sources src/styles/css:./styles",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test:spec": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "concurrently \"stencil build --watch\" \"storybook dev --no-open -p 6006\"",
    "storybook-nb": "npx storybook dev -p 6006",
    "storybook-dev": "concurrently \"stencil build --debug --dev --watch\" \"npx storybook dev --no-open -p 6006\"",
    "build-storybook": "storybook build",
    "story": "yarn start & yarn storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@stencil/angular-output-target": "^0.9.0",
    "@stencil/core": "^4.21.0",
    "@stencil/react-output-target": "^0.7.1",
    "@stencil/sass": "^3.0.12",
    "@storybook/addon-a11y": "^8.3.2",
    "@storybook/addon-actions": "^8.3.2",
    "@storybook/addon-essentials": "^8.3.2",
    "@storybook/addon-links": "^8.3.2",
    "@storybook/builder-vite": "^8.3.2",
    "@storybook/web-components": "^8.3.2",
    "@storybook/web-components-vite": "^8.3.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "clean-css-cli": "^5.6.3",
    "concurrently": "^9.0.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lit": "^3.2.0",
    "puppeteer": "^23.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.79.2",
    "storybook": "^8.3.2",
    "vite": "^5.4.6"
  },
  "license": "MIT",
  "url": "https://github.com/UKHO/admiralty-design-system",
  "repository": "git://github.com/UKHO/admiralty-design-system.git"
}
