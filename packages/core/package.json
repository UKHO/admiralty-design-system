{
  "name": "@ukho/admiralty-core",
  "version": "0.0.6",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/admiralty-core/admiralty-core.esm.js",
  "files": [
    "dist/",
    "loader/",
    "styles/"
  ],
  "scripts": {
    "build": "yarn build.css && stencil build",
    "build.css": "yarn css.sass && yarn css.minify",
    "css.minify": "cleancss -O2 -o styles/core.css styles/core.css",
    "css.sass": "sass --embed-sources src/scss/core.scss:./styles/core.css",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test:spec": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "concurrently \"stencil build --watch\" \"cross-env BROWSER=none npx start-storybook -p 6006\"",
    "storybook-nb": "npx start-storybook -p 6006",
    "storybook-dev": "concurrently \"stencil build --debug --dev --watch\" \"cross-env BROWSER=none npx start-storybook -p 6006\"",
    "build-storybook": "build-storybook",
    "story": "yarn start & yarn storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@stencil/angular-output-target": "^0.6.0",
    "@stencil/core": "^3.2.0",
    "@stencil/sass": "^3.0.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-styling": "^0.3.2",
    "@storybook/html": "^6.5.16",
    "@types/jest": "^27.0.3",
    "@types/node": "^18.15.11",
    "babel-loader": "^9.1.2",
    "clean-css-cli": "^5.6.2",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "puppeteer": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "sass-loader": "13.2.2",
    "style-loader": "^2.0.0"
  },
  "license": "MIT",
  "url": "https://github.com/UKHO/admiralty-design-system",
  "repository": "git://github.com/UKHO/admiralty-design-system.git"
}
