{
  "name": "@ukho/admiralty-core",
  "version": "0.12.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/admiralty-core/admiralty-core.esm.js",
  "files": [
    "dist/",
    "loader/",
    "styles/",
    "docs/"
  ],
  "scripts": {
    "build": "yarn build.css && stencil build",
    "build.css": "yarn css.sass && yarn css.minify",
    "css.minify": "cleancss -O2 -o styles/core.css styles/core.css",
    "css.sass": "sass --embed-sources src/scss/core.scss:./styles/core.css",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test:spec": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "concurrently \"stencil build --watch\" \"storybook dev --no-open -p 6006\"",
    "storybook-nb": "npx storybook dev -p 6006",
    "storybook-dev": "concurrently \"stencil build --debug --dev --watch\" \"npx storybook dev --no-open -p 6006\"",
    "build-storybook": "storybook build",
    "story": "yarn start & yarn storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@stencil/angular-output-target": "^0.7.1",
    "@stencil/core": "^3.4.2",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/sass": "^3.0.5",
    "@storybook/addon-a11y": "^7.6.7",
    "@storybook/addon-actions": "^7.6.7",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/web-components": "^7.6.7",
    "@storybook/web-components-vite": "^7.6.7",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.17.0",
    "clean-css-cli": "^5.6.2",
    "concurrently": "^8.2.1",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-environment-node": "^27.5.1",
    "lit": "^2.7.6",
    "puppeteer": "^20.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.64.1",
    "storybook": "^7.6.7",
    "vite": "^4.4.9"
  },
  "license": "MIT",
  "url": "https://github.com/UKHO/admiralty-design-system",
  "repository": "git://github.com/UKHO/admiralty-design-system.git"
}
